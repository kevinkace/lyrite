    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway|Slabo+27px|Bitter:400,700|Lato:400,700">
</head>
<body>

    <div id="mount"></div>

    <script>
        const q = {};

        function desanitize(path) {
            return path.replace(/~and~/g, "&")
        }

        function checkRedirect() {

            if(!location.search) {
                return;
            }

            location.search.slice(1).split("&").forEach(function(params) {
                const [ key, value ] = params.split("=");

                q[key] = desanitize(value);
            });

            if(q.p === undefined) {
                return;
            }

            history.replaceState(null, null,
                location.pathname.slice(0, -1) + (q.p || "") +
                (q.q ? ("?" + q.q) : "") +
                location.hash
            );
        }

        checkRedirect();

        const ver = <%= JSON.stringify(htmlWebpackPlugin.options.ver) %>;
    </script>

